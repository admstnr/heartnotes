/* selectivity */

.selectivity-multiple-input-container
  border none
  padding 0
  background transparent

.selectivity-multiple-input, input[type='text'].selectivity-multiple-input
  border 1px solid #ccc
  border-radius 4px
  padding .5em .6em
  height auto
  
.selectivity-multiple-selected-item
  white-space normal